# euclide's algorithm in ssa
# it is a fairly interesting
# ssa program because of the
# swap of b and a

export
function $test() {
@start

@loop
	%a =w phi @start 380, @loop %r
	%b =w phi @start 747, @loop %a
	%r =w rem %b, %a
	jnz %r, @loop, @end

@end
	storew %a, $a
	ret
}

